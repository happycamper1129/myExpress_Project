{
  "apiEndpoints": {
    "api": {
      "host": "*.com"
    }
  },
  "serviceEndpoints": {
    "backend": { "url": "http://localhost" }
  },
  "policies": ["proxy"],
  "pipelines": {
    "apiPipeline": {
      "apiEndpoints": [ "api" ],
      "policies": [
        {
          "proxy": [
            { "action": { "serviceEndpoint": "backend" } }
          ]
        }
      ]
    }
  },
  "http": {
    "port": 5997
  }
}
